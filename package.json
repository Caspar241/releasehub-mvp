  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "bash scripts/dev-clean.sh",
    "dev:safe": "killall -9 node 2>/dev/null || true && sleep 1 && rm -rf .next node_modules/.cache && npm run build && npm run dev",
    "dev:turbo": "next dev --turbo",
    "dev:build": "npm run build && npm run dev",
    "health": "bash scripts/health-check.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate"
  },
