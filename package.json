    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.553.0",
    "micro": "^10.0.1",
    "next": "14.2.15",
    "react": "18.3.1",
    "react-day-picker": "^9.11.1",
    "react-dom": "18.3.1",
    "recharts": "^3.3.0",
    "stripe": "^17.3.1"
  },
  "devDependencies": {
    "@types/micro": "^7.3.7",
