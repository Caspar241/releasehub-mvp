{
  "name": "releasehub-mvp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "bash scripts/dev-clean.sh",
    "dev:safe": "killall -9 node 2>/dev/null || true && sleep 2 && rm -rf .next && npm run dev",
    "dev:build": "npm run build && npm run dev",
    "health": "bash scripts/health-check.sh",
    "predev": "mkdir -p .next/server && test -f .next/server/middleware-manifest.json || echo '{\"sortedMiddleware\":[],\"middleware\":{},\"functions\":{},\"version\":2}' > .next/server/middleware-manifest.json",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate"
  },
  "volta": {
    "node": "20.18.0",
    "npm": "10.8.2"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "@supabase/supabase-js": "^2.46.1",
    "framer-motion": "^12.23.24",
    "micro": "^10.0.1",
    "next": "^14.2.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stripe": "^17.3.1"
  },
  "devDependencies": {
    "@types/micro": "^7.3.7",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.16",
    "eslint": "^9",
    "eslint-config-next": "15.0.2",
    "postcss": "^8",
    "prisma": "^6.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
